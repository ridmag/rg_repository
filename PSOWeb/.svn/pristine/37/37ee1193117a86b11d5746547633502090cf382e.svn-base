package com.itelasoft.pso.ws.value;

import java.util.Date;

import com.itelasoft.pso.beans.ProgramType;

public class Program {
	public transient com.itelasoft.pso.beans.Program program;


	public Long getId() {
		return program.getId();
	}

	public void setId(Long id) {
		program.setId(id);
	}

	public String getName() {
		return program.getName();
	}

	public void setName(String name) {
		program.setName(name);
	}

	public Date getStartDate() {
		return program.getStartDate();
	}

	public void setStartDate(Date startDate) {
		program.setStartDate(startDate);
	}

	public Date getEndDate() {
		return program.getEndDate();
	}

	public void setEndDate(Date endDate) {
		program.setEndDate(endDate);
	}

	public String getType() {
		return program.getType().getName();
	}

	public void setType(String type) {
		
	}

	public String getStatus() {
		return program.getStatus();
	}

	public void setStatus(String status) {
		program.setStatus(status);
	}


	public Program(com.itelasoft.pso.beans.Program program) {
		this.program = program;
	}
}
