<?xml version="1.0" encoding="ISO-8859-1" ?>
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:fo="http://www.w3.org/1999/XSL/Format">
	
	
<body >
<table   border="0" cellspacing="0" cellpadding="0" width="745" style="border-collapse:collapse">
	#foreach( $studentRow in $students )
		<tr style='height:64.05px'>
			#foreach( $student in $studentRow )
				<td width="242" valign="top" style="width:181.4px;padding:2.85px 0in 2.85px 0in;height:64.05px">
					<p  style="margin-top:0in;margin-right:7.1px;margin-bottom:0in;margin-left:7.1px;margin-bottom:.0001px">
					<span style="font-size:10.0px;font-family: 'Arial','sans-serif';">$student.contact.name</span></p>
					<p  style="margin-top:0in;margin-right:7.1px;margin-bottom:0in;margin-left:7.1px;margin-bottom:.0001px;font-size: 8.0px;font-family: 'Arial','sans-serif';">
					#if($student.mailAddressDefault)
					$student.contact.streetAddress<br />
					$student.contact.city<br />
					$student.contact.state $student.contact.postCode
				#end #if(not $student.mailAddressDefault)
				$student.mailingAddress.streetAddress
				<br /> $student.mailingAddress.city
				<br /> $student.mailingAddress.state
				$student.mailingAddress.postCode
				#end
				</p></td> 
			#end
		</tr>
	#end
</table>
</body>
</html>