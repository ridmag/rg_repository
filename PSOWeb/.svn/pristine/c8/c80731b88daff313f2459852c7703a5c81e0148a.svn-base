		<property name="hibernateProperties">
			<props>
				<prop key="hibernate.dialect">org.hibernate.dialect.PostgreSQLDialect</prop>
				<prop key="hibernate.cache.provider_class">org.hibernate.cache.NoCacheProvider</prop>
				<prop key="hibernate.max_fetch_depth">1</prop>
				<!--<prop key="hibernate.default_schema">ivetdb</prop> -->
				<prop key="hibernate.show_sql">false</prop>
				<prop key="hbm2ddl.keywords">auto-quote</prop>
				<prop key="metadata.hibernate.dialect">com.jaspersoft.hibernate.dialect.PostgresqlNoBlobDialect
				</prop>
				<prop key="hibernate.search.default.directory_provider">filesystem</prop>
				<prop key="hibernate.search.default.indexBase">/var/tmp/lucene</prop>
				<prop key="hibernate.search.default.locking_strategy">none</prop>
				<prop key="hibernate.search.default.indexmanager">near-real-time</prop>
			</props>
		</property>
