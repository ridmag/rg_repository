<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ice="http://www.icesoft.com/icefaces/component"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.prime.com.tr/ui"
	xmlns:comp="http://java.sun.com/jsf/composite/comp">
<h:body>
	<ui:composition template="/PageTemplate.xhtml">
		<ui:define name="pageTitle">Staff</ui:define>
		<ui:define name="PageTitleIcon">
			<img src="images/icons/home.png" />
		</ui:define>
		<ui:define name="pageHeading">Staff Manager</ui:define>
		<ui:define name="pageDescription">Welcome to the PSO Management. Follow instructions below to get started.</ui:define>
		<ui:define name="pageContent">
			<ice:form id="formStaff">
				<ice:dataTable value="#{invoiceManagerModel.unbilledTxn}" var="txn">
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Id" />
						</f:facet>
						<ice:outputText value="#{txn.id}" />
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="#{msgs['student_label']} Id" />
						</f:facet>
						<ice:outputText value="#{txn.studentId}" />
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Program Id" />
						</f:facet>
						<ice:outputText value="#{txn.programEvent.program.id}" />
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Invoice Id" />
						</f:facet>
						<ice:outputText value="#{txn.invoiceId}" />
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Txn Type" />
						</f:facet>
						<ice:outputText value="#{txn.transactionType.id}" />
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Amount" />
						</f:facet>
						<ice:outputText value="#{txn.amount}" />
					</ice:column>
				</ice:dataTable>
			</ice:form>
		</ui:define>

	</ui:composition>

</h:body>
</html>